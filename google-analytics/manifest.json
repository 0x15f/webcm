{
  "name": "Google Analytics",
  "description": "Google Analytics",
  "namespace": "google-analytics",
  "icon": "assets/google-analytics.svg",
  "categories": ["Analytics"],
  "provides": ["events"],
  "fields": {
    "account_id": {}
  },
  "allowCustomFields": true,
  "permissions": [
    {
      "permission": "clientFetch",
      "explanation": "Google Analytics uses client fetch to attribute sessions more accurately",
      "required": false
    },
    {
      "permission": "run_client_js",
      "explanation": "If you're using GA Audiences",
      "required": true
    }
  ]
}
